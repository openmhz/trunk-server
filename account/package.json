{
  "name": "react-passport-redux-example",
  "version": "1.0.0",
  "description": "Demonstration of PassportJS authentication in React w/ Redux.",
  "main": "devStart.js",
  "scripts": {
    "build:lib": "babel src/server --out-dir lib/server",
    "build:umd": "webpack --config webpack/webpack.config.dev.js",
    "build:umd:min": "webpack --config webpack/webpack.config.prod.js",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "postinstall": "npm run build",
    "clean": "rimraf lib public/assets",
    "server": "better-npm-run server",
    "server:dev": "better-npm-run server:dev",
    "start": "npm run server",
    "start:dev": "npm run server:dev",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "betterScripts": {
    "server": {
      "command": "node lib/server/index.js",
      "env": {
        "NODE_ENV": "production",
        "MIN_EXT": ".min"
      }
    },
    "server:dev": {
      "command": "node devStart.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "7.3.1",
    "axios": "0.21.1",
    "bcrypt-nodejs": "0.0.3",
    "better-npm-run": "0.1.1",
    "body-parser": "1.18.3",
    "connect-mongo": "2.0.3",
    "connected-react-router": "6.3.1",
    "csv-parse": "4.3.3",
    "d3-time-format": "2.1.3",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "fs-extra": "7.0.1",
    "lodash": "4.17.11",
    "mongoose": "5.4.15",
    "multer": "1.4.1",
    "node-mailjet": "3.3.1",
    "node-uuid": "1.4.8",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-redux": "6.0.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-stripe-elements": "2.0.3",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.85.0",
    "storybook": "1.0.0",
    "stripe": "6.25.1"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-1": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/react": "4.1.13",
    "babel-loader": "8.0.5",
    "babel-plugin-lodash": "3.3.4",
    "detect-node": "^2.0.4",
    "redux-logger": "3.0.6",
    "rimraf": "2.6.3",
    "webpack": "4.29.5",
    "webpack-cli": "3.2.3",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3"
  }
}
