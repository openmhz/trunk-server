    "redux-logger": "3.0.6",
    "rimraf": "2.6.3",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-less": "2.4.1",
    "style-loader": "0.23.1",
    "webpack": "4.29.5",
    "webpack-cli": "3.2.3",
    "webpack-dev-middleware": "^3.6.0",
    "webpack-hot-middleware": "2.24.3"
  }
