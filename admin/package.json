{
  "name": "react-passport-redux-example",
  "version": "1.0.0",
  "description": "Demonstration of PassportJS authentication in React w/ Redux.",
  "main": "devStart.js",
  "scripts": {
    "build:lib": "babel src/server --out-dir lib/server",
    "build:umd": "webpack --config webpack/webpack.config.dev.js",
    "build:umd:min": "webpack --config webpack/webpack.config.prod.js",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "postinstall": "npm run build",
    "clean": "rimraf lib public/assets",
    "server": "better-npm-run server",
    "server:dev": "better-npm-run server:dev",
    "start": "npm run server",
    "start:dev": "npm run server:dev"
  },
  "betterScripts": {
    "server": {
      "command": "node lib/server/index.js",
      "env": {
        "NODE_ENV": "production",
        "MIN_EXT": ".min"
      }
    },
    "server:dev": {
      "command": "node devStart.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "7.3.1",
    "@vx/axis": "^0.0.184",
    "@vx/curve": "^0.0.182",
    "@vx/glyph": "^0.0.183",
    "@vx/group": "^0.0.183",
    "@vx/legend": "^0.0.183",
    "@vx/mock-data": "^0.0.185",
    "@vx/responsive": "0.0.184",
    "@vx/scale": "0.0.182",
    "@vx/shape": "0.0.184",
    "@vx/tooltip": "0.0.184",
    "axios": "^0.21.1",
    "babel-loader": "8.0.5",
    "bcrypt-nodejs": "^0.0.3",
    "better-npm-run": "^0.1.0",
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.1",
    "connected-react-router": "6.3.1",
    "csv-parse": "^4.3.3",
    "d3-time-format": "^2.1.3",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "fs-extra": "^7.0.1",
    "lodash": "4.17.11",
    "mongoose": "^5.4",
    "multer": "^1.3.0",
    "node-mailjet": "*",
    "node-uuid": "^1.4.8",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-redux": "^6.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.85.0"
  },
