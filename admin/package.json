    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-1": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-lodash": "3.3.4",
    "detect-node": "2.0.4",
    "redux-logger": "^3.0.6",
